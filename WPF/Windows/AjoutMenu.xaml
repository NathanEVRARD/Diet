<Window x:Class="WPF.Windows.AjoutMenu"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPF.Windows"
        mc:Ignorable="d"
        Title="AjoutMenu" Height="450" Width="800">
    <Grid>
        <StackPanel Orientation="Horizontal">
            <TextBlock FontSize="24" Text="{Binding Client.Nom}" Margin="5"></TextBlock>
            <TextBlock FontSize="24" Text="{Binding Client.Prenom}" Margin="5"></TextBlock>
        </StackPanel>
    </Grid>
</Window>
